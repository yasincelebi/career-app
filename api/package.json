    "apollo-server-core": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.2",
    "graphql": "^16.2.0",
    "helmet": "^4.6.0",
